{
    "name": "smsapi/monolog-smsapi",
    "description": "A Monolog Handler for SMSAPI",
    "license": "Apache-2.0",
    "homepage": "http://smsapi.pl",
    "version": "0.2.2",
    "keywords": [
        "monolog",
        "sms",
        "smsapi",
        "handler",
        "formatter"
    ],
    "require": {
        "php": "5.6.*",
        "monolog/monolog": "^1.19",
        "smsapi.pl/php-client": "^1",
        "beberlei/assert": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "MonologSmsapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonologSmsapi\\Tests\\": "tests/"
        }
    }
}
